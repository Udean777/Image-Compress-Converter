		"@prisma/client": "^7.2.0",
		"@stripe/stripe-js": "^8.6.1",
		"@types/bcryptjs": "^3.0.0",
		"archiver": "^7.0.1",
		"arctic": "^3.7.0",
		"bcryptjs": "^3.0.3",
		"canvas-confetti": "^1.9.4",
		"chart.js": "^4.5.1",
		"date-fns": "^4.1.0",
		"googleapis": "^170.1.0",
