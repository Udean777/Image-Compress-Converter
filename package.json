{
	"name": "mini-saas-vivnio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"docker:up": "docker-compose up -d",
		"docker:down": "docker-compose down",
		"docker:logs": "docker-compose logs -f",
		"docker:build": "docker-compose up -d --build",
		"docker:clean": "docker-compose down -v",
		"db:seed": "bunx tsx prisma/seed/seed-pricing.ts && bunx tsx prisma/seed/seed-admin.ts",
		"stripe:listen": "stripe listen --forward-to localhost:1234/api/webhook/stripe",
		"cleanup:imports": "bun scripts/remove-unused-imports.ts"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@sveltejs/adapter-auto": "^7.0.0",
